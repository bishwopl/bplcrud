{
    "name": "bishwopl/bplcrud",
    "description": "Simple crud package and form generator using laminas-form and doctrine-orm",
    "type": "library",
    "homepage": "https://github.com/bishwopl/bplcrud",
    "keywords": [
        "crud",
        "form-generator",
        "laminas-form",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Bishwo Prasad Lamichhane",
            "email": "bishwo.prasad@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/orm": "^2.8",
        "doctrine/doctrine-module": "^4.1",
        "laminas/laminas-form": "^2.15",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-paginator": "^2.9",
        "laminas/laminas-code": "^4.0",
        "laminas/laminas-view": "^2.12"
    },
    "autoload": {
        "psr-4": { 
            "BplCrud\\": "src"
        }
    }
}
